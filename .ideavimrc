source ~/.vimrc

set surround

map <leader>r <Action>(RecentFiles)
map <leader>b <Action>(Switcher)
map <leader>o <Action>(GotoClass)
map <leader>f <Action>(GotoFile)
map <leader>g <Action>(FindInPath)
map <leader>s <Action>(FileStructurePopup)

map <C-A-o> <Action>(OptimizeImports)

map <C-r> <Action>(Run)
map <C-S-r> <Action>(Debug)

imap JJ <Action>(EditorCompleteStatement)
